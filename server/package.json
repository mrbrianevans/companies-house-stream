{
  "name": "companies-house-stream-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --build --watch",
    "clean": "tsc --build --clean",
    "start": "node --experimental-specifier-resolution=node dist",
    "start:watch": "nodemon dist",
    "status": "node --experimental-specifier-resolution=node dist/testStreamStatus.js",
    "stream-pub": "node --experimental-specifier-resolution=node dist/redis/streamToRedis.js",
    "stream-sub": "node dist/redis/streamFromRedis.js",
    "api": "node --experimental-specifier-resolution=node dist/restApi/server.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.8",
    "@types/pg": "^8.6.5",
    "@types/ws": "^8.5.3",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.9.0",
    "pg": "^8.7.3",
    "pino": "^8.4.2",
    "redis": "^4.2.0",
    "ws": "^8.8.1"
  }
}
