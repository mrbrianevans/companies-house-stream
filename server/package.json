  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "JSONStream": "^1.3.5",
    "mongodb": "^4.5.0",
    "pg": "^8.7.3",
    "pino": "^7.11.0",
    "redis": "^4.0.6",
    "ws": "^8.5.0"
  }
